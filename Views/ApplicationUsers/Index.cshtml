@model IEnumerable<AvengersUnityIdentity.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}
    <div class="jumbotron background">

        <h2>Utilisateurs</h2>

        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Civil.Prenom)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Organisation.Nom_Organisation)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>

                </th>
            </tr>

            @foreach (var item in Model)
            {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Civil.Prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Organisation.Nom_Organisation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>

            <td>
                @Html.ActionLink("Consulter", "Details", new { id = item.Id })
            </td>
        </tr>
            }

        </table>
    </div>